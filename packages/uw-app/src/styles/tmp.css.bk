	position: absolute;
	width: 100%;
	padding-top: 1.5rem;
}

.floating-button__open-icon .floating-button__bar {
	width: 40%;
	margin: 0 auto;
	height: 4px;
	margin-bottom: 4px;
	border-radius: .125rem;
	opacity: 1;
}

@media (min-width:25em) {
	.floating-button__open-icon .floating-button__bar {
		height: 5px;
		margin-bottom: 6px
	}
}

.floating-button__open-icon .floating-button__bar:nth-child(1) {
	transition: .3s all -.05s
}

.floating-button__open-icon .floating-button__bar:nth-child(2) {
	transition: .3s all 0s
}

.floating-button__open-icon .floating-button__bar:nth-child(3) {
	transition: .3s all .04999999999999999s
}

.sidebar-open .floating-button__open-icon .floating-button__bar {
	opacity: 0;
	transform: translate(.5rem)
}

.floating-button__close-icon {
	position: absolute;
	width: 100%;
	padding-top: 2.5rem;
}

.floating-button__close-icon .floating-button__bar {
	width: 0%;
	height: .4rem;
	margin: -.4rem auto 0;
	border-radius: .2rem;
	opacity: 0;
	transition: .5s all;
}

.floating-button__close-icon .floating-button__bar:nth-child(1),
.floating-button__close-icon .floating-button__bar:nth-child(3) {
	transform: rotate(45deg)
}

.floating-button__close-icon .floating-button__bar:nth-child(2) {
	transform: rotate(-45deg)
}

