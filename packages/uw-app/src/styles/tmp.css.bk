	background: #0e141f;
	background: linear-gradient(#0e141f, #0a0e16);
	border: 1px solid #1c293f;
	box-shadow: 0 .33rem 0 #080c13, 0 .5rem .2rem -.1rem #000;
	text-shadow: 0 1px #070a0f, 0 2px #070a0f;
}

.clickable__black:hover:not(.is-active) {
	background: #151e2f;
	background: linear-gradient(#151e2f, #0b0f17);
	box-shadow: 0 .33rem 0 #080b11, 0 .5rem .2rem -.1rem #000
}

.clickable__black:active {
	top: .33rem;
	box-shadow: 0 0 0 #080c13, 0 .15rem .2rem -.1rem #000
}

.clickable__black:active::after {
	bottom: -6px
}

