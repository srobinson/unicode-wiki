.clickable,
.button {
	display: block;
	color: #a89e8a;
	font-weight: normal;
	border-radius: .25rem;
	top: 0;
	transition: top .1s, box-shadow .1s;
}

.clickable.is-active,
.button.is-active {
	pointer-events: none
}

.clickable:hover:not(.is-active),
.button:hover:not(.is-active) {
	cursor: pointer
}

.clickable::after,
.button::after {
	content: '';
	display: block;
	position: absolute;
	top: -6px;
	left: -6px;
	right: -6px;
	bottom: calc(-6px - 0.33rem);
	border-radius: .5rem;
	border: 1px solid #ffcb36;
	transition: .1s opacity, .1s bottom;
	opacity: 0
}

.clickable:active,
.button:active,
.clickable:focus,
.button:focus {
	outline: none;
}

.clickable:active::after,
.button:active::after,
.clickable:focus::after,
.button:focus::after {
	opacity: 1
}

