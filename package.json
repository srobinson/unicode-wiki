{
  "name": "unicode-wiki-workspace",
  "version": "0.1.8",
  "description": "Unicode Explorer workspace home",
  "author": "Stuart Robinson",
  "homepage": "https://unicode.wiki/",
  "repository": "https://github.com/srobinson/unicode-wiki",
  "bugs": "https://github.com/srobinson/unicode-wiki/issues",
  "keywords": [
    "unicode",
    "wikipedia",
    "characters",
    "fonts",
    "explore",
    "search"
  ],
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "yarn clean && lerna bootstrap",
    "build:app": "lerna run build:app",
    "clean": "lerna clean --yes && yarn clean:dist",
    "clean:dist": "rimraf packages/**/build && rimraf packages/**/dist",
    "compile": "cd packages/uw-$P && yarn prepare && cd ../..",
    "commit": "npx git-cz && yarn git:push",
    "commit:retry": "npx git-cz --retry && yarn git:push",
    "c": "yarn commit",
    "cr": "yarn commit:retry",
    "dev": "lerna run --parallel dev",
    "git:push": "git push origin HEAD",
    "lint": "lerna run lint",
    "prepare": "lerna run prepare",
    "seed": "lerna run seed --concurrency 20 --stream",
    "start:api": "lerna run start:api",
    "start:app": "lerna run start:app",
    "test": "yarn lint && NODE_ENV=test jest --runInBand --forceExit --env=node --colors",
    "test:seed": "lerna run test:seed",
    "update-version": "lerna publish --skip-git --skip-npm --force-publish '*'",
    "version": "./version.sh",
    "watch": "lerna run watch --parallel"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json"
    ],
    "setupFiles": [
      "./test-shim.js",
      "./test-setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "apollo-boost": "0.1.22",
    "apollo-datasource": "0.2.1",
    "apollo-datasource-rest": "0.2.1",
    "apollo-server": "2.3.1",
    "apollo-server-testing": "2.3.1",
    "async": "2.6.1",
    "axios": "0.18.0",
    "babel-plugin-emotion": "9.2.11",
    "body-parser": "1.18.3",
    "compression": "1.7.3",
    "connected-react-router": "4.5.0",
    "cookie-parser": "1.4.3",
    "core-js": "2.5.7",
    "cors": "2.8.4",
    "dotenv": "6.0.0",
    "downshift": "3.1.5",
    "elasticsearch": "15.2.0",
    "emotion": "9.2.10",
    "emotion-theming": "9.2.9",
    "express": "4.16.3",
    "express-async-errors": "3.0.0",
    "graphql": "14.0.2",
    "history": "4.7.2",
    "hoist-non-react-statics": "3.2.1",
    "mongodb": "3.1.6",
    "mongoose": "5.3.0",
    "mongoose-paginate-v2": "1.0.12",
    "node-fetch": "2.3.0",
    "ora": "3.0.0",
    "polished": "2.2.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-emotion": "9.2.12",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-router-redux": "4.0.8",
    "react-waypoint": "8.0.3",
    "redux": "4.0.1",
    "redux-devtools-extension": "2.13.5",
    "testdouble": "3.9.1",
    "uuid": "3.3.2",
    "winston": "3.1.0",
    "xml2js": "0.4.19"
  },
  "devDependencies": {
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "@types/async": "2.0.49",
    "@types/axios": "0.14.0",
    "@types/body-parser": "1.17.0",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "1.4.1",
    "@types/cors": "2.8.4",
    "@types/dotenv": "4.0.3",
    "@types/elasticsearch": "5.0.30",
    "@types/enzyme-adapter-react-16": "1.0.3",
    "@types/express": "4.16.0",
    "@types/graphql": "14.0.3",
    "@types/history": "4.7.2",
    "@types/hoist-non-react-statics": "3.0.1",
    "@types/jest": "23.3.8",
    "@types/mongoose": "5.2.17",
    "@types/node": "8.0.0",
    "@types/node-fetch": "2.1.4",
    "@types/ora": "1.3.4",
    "@types/react": "16.4.14",
    "@types/react-dom": "16.0.8",
    "@types/react-redux": "6.0.9",
    "@types/react-router": "4.0.32",
    "@types/react-router-dom": "4.3.1",
    "@types/react-router-redux": "5.0.16",
    "@types/redux-mock-store": "1.0.0",
    "@types/rimraf": "2.0.2",
    "@types/supertest": "2.0.6",
    "@types/uuid": "3.4.4",
    "@types/xml2js": "0.4.3",
    "copyfiles": "2.1.0",
    "cz-conventional-changelog": "2.1.0",
    "deep-diff": "1.0.2",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "enzyme-to-json": "3.3.5",
    "husky": "1.3.1",
    "jest": "23.6.0",
    "jest-cli": "23.3.0",
    "lint-staged": "8.1.0",
    "nock": "10.0.4",
    "oao": "1.5.1",
    "prettier": "1.14.0",
    "prettier-tslint": "0.4.0",
    "react-scripts-ts": "3.1.0",
    "redux-mock-store": "1.5.3",
    "rimraf": "2.6.2",
    "semantic-release": "15.13.2",
    "supertest": "3.3.0",
    "ts-jest": "23.10.4",
    "ts-loader": "5.2.2",
    "ts-node-dev": "1.0.0-pre.30",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-config-security": "1.14.0",
    "typescript": "3.0.3"
  },
  "sideEffects": false
}
