{
  "name": "unicode-wiki-workspace",
  "version": "0.0.1",
  "description": "Unicode Explorer workspace home",
  "author": "Stuart Robinson",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build:app": "lerna run build:app",
    "clean": "lerna clean",
    "clean:dist": "rimraf packages/**/build && rimraf packages/**/dist",
    "dev": "yarn kill3000 && yarn kill3001 && lerna run --parallel dev",
    "kill3000": "lsof -i TCP:3000 | grep LISTEN | awk '{print $2}' | xargs kill -9",
    "kill3001": "lsof -i TCP:3001 | grep LISTEN | awk '{print $2}' | xargs kill -9",
    "lint": "lerna run lint",
    "prepare": "yarn clean:dist && lerna run prepare",
    "seed": "lerna run seed",
    "start:api": "lerna run start:api",
    "start:app": "lerna run start:app",
    "test": "jest",
    "update-version": "lerna publish --skip-git --skip-npm --force-publish '*'",
    "watch": "lerna run --parallel watch"
  },
  "dependencies": {
    "async": "2.6.1",
    "axios": "0.18.0",
    "babel-plugin-emotion": "9.2.11",
    "body-parser": "1.18.3",
    "connected-react-router": "4.5.0",
    "cookie-parser": "1.4.3",
    "core-js": "2.5.7",
    "cors": "2.8.4",
    "dotenv": "6.0.0",
    "emotion-theming": "9.2.9",
    "emotion": "9.2.10",
    "express-async-errors": "3.0.0",
    "express": "4.16.3",
    "history": "4.7.2",
    "mongodb": "3.1.6",
    "mongoose-paginate-v2": "1.0.12",
    "mongoose": "5.3.0",
    "ora": "3.0.0",
    "polished": "2.2.0",
    "react-dom": "16.5.2",
    "react-emotion": "9.2.12",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-router-redux": "4.0.8",
    "react-router": "4.3.1",
    "react-waypoint": "8.0.3",
    "react": "16.5.2",
    "redux-devtools-extension": "2.13.5",
    "redux": "4.0.1",
    "uuid": "3.3.2",
    "winston": "3.1.0",
    "xml2js": "0.4.19"
  },
  "devDependencies": {
    "@types/async": "2.0.49",
    "@types/axios": "0.14.0",
    "@types/body-parser": "1.17.0",
    "@types/cookie-parser": "1.4.1",
    "@types/cors": "2.8.4",
    "@types/dotenv": "4.0.3",
    "@types/express": "4.16.0",
    "@types/history": "4.7.2",
    "@types/jest": "23.3.8",
    "@types/mongoose": "5.2.17",
    "@types/node": "8.0.0",
    "@types/ora": "1.3.4",
    "@types/react-dom": "16.0.8",
    "@types/react-redux": "6.0.9",
    "@types/react-router-dom": "4.3.1",
    "@types/react-router-redux": "5.0.16",
    "@types/react-router": "4.0.32",
    "@types/react": "16.4.14",
    "@types/supertest": "2.0.6",
    "@types/uuid": "3.4.4",
    "@types/xml2js": "0.4.3",
    "copyfiles": "2.1.0",
    "deep-diff": "1.0.2",
    "jest-cli": "23.3.0",
    "jest": "23.6.0",
    "prettier-tslint": "0.4.0",
    "prettier": "1.14.0",
    "react-scripts-ts": "3.1.0",
    "rimraf": "2.6.2",
    "supertest": "3.3.0",
    "ts-jest": "23.10.4",
    "ts-loader": "5.2.2",
    "ts-node-dev": "1.0.0-pre.30",
    "tslint-config-prettier": "1.15.0",
    "tslint": "5.11.0",
    "typescript": "3.0.3"
  },
  "sideEffects": false
}
